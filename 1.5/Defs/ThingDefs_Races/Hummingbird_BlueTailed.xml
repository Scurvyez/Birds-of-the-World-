          <soundDeath>Pawn_Chick_Call</soundDeath>
          <soundCall>Pawn_Chick_Call</soundCall>
          <soundAngry>Pawn_Chick_Call</soundAngry>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>0.1666</minAge>
          <soundWounded>Pawn_BTHummer_Call</soundWounded>
          <soundCall>Pawn_BTHummer_Call</soundCall>
          <soundAngry>Pawn_BTHummer_Call</soundAngry>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>0.5</minAge>
          <soundWounded>Pawn_BTHummer_Call</soundWounded>
          <soundCall>Pawn_BTHummer_Call</soundCall>
          <soundAngry>Pawn_BTHummer_Call</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
    </race>
    <tradeTags>
      <li>AnimalUncommon</li>
    </tradeTags>
    <tools>
      <li>
        <label>claws</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>3</power>
        <cooldownTime>1.5</cooldownTime>
        <linkedBodyPartsGroup>Feet</linkedBodyPartsGroup>
        <chanceFactor>0.5</chanceFactor>
      </li>
      <li>
        <label>beak</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>5</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>Beak</linkedBodyPartsGroup>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>1</power>
        <cooldownTime>1.5</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <comps>
      <li Class="CompProperties_EggLayer">
        <eggUnfertilizedDef>EggBlueTailedHummerUnfertilized</eggUnfertilizedDef>
        <eggFertilizedDef>EggBlueTailedHummerFertilized</eggFertilizedDef>
